[View]
PName=V_RECEIVE_ITEM_DETAIL
LName=入庫商品明細ビュー
Comment=入庫商品明細の履歴を含めたビュー。月次在庫バッチで利用する。\n・REGIST_ACCOUNT_FLAG\n⇒経理連携する取引分類は「1」を設定\n⇒取引分類が調整かつ資産区分が済の場合は「1」を設定\n⇒処理種別が小割、加工の場合は「1」を設定
Query=( \n    SELECT\n        RID.CORP_CODE, \n        RID.PROCESS_CLASS_CODE, \n        RID.RECEIVE_NO, \n        IFNULL(R.REVISION_NO, IFNULL(S.REVISION_NO, 0)) AS REVISION_NO, \n        RID.DETAIL_NO, \n        RID.STOCK_NO, \n        RID.STOCK_HISTORY_NO, \n        RID.SRC_PROCESS_CLASS_CODE, \n        RID.SRC_REPORT_NO, \n        RID.SRC_DETAIL_NO, \n        RID.LATEST_STOCK_NO, \n        RID.LATEST_STOCK_HISTORY_NO, \n        RID.SRC_STOCK_NO, \n        RID.SRC_STOCK_HISTORY_NO, \n        RID.BASE_COST_AMOUNT, \n        RID.BASE_COST_ADD_AMOUNT, \n        RID.BASE_COST_CHARGE_AMOUNT, \n        RID.CREATE_DATETIME, \n        RID.CREATE_USER_CODE, \n        RID.UPDATE_DATETIME, \n        RID.UPDATE_USER_CODE, \n        STK.STOCK_ID, \n        STK.IKO_IN_DT, \n        STK.ASSET_TYPE_CODE AS STK_ASSET_TYPE_CODE, \n        STK.STOCK_QUANTITY, \n        IFNULL(R.PROCESS_CLASS_CODE, IFNULL(SR.PROCESS_CLASS_CODE, STKR.PROCESS_CLASS_CODE)) AS RCV_PROCESS_CLASS_CODE, \n        IFNULL(R.RECEIVE_NO, IFNULL(SR.RECEIVE_NO, STK.RECEIVE_NO)) AS RCV_RECEIVE_NO, \n        IFNULL(R.REVISION_NO, IFNULL(S.REVISION_NO, 0)) AS RCV_REVISION_NO, \n        IFNULL(RIS.DETAIL_NO, IFNULL(SRIS.DETAIL_NO, STK.RECEIVE_ITEM_SUMMARY_DETAIL_NO)) AS RCV_DETAIL_NO, \n        IFNULL(R.RB_TYPE, IFNULL(SR.RB_TYPE, \q0\q)) AS RB_TYPE, \n        IFNULL(R.ASSET_TYPE_CODE, SR.ASSET_TYPE_CODE) AS RCV_ASSET_TYPE_CODE, \n        IFNULL(R.BUY_RECORD_DATE, SR.BUY_RECORD_DATE) AS BUY_RECORD_DATE, \n        IFNULL(R.CREATE_DATETIME, SR.CREATE_DATETIME) AS RCV_CREATE_DATETIME, \n        CASE \n            WHEN RID.PROCESS_CLASS_CODE IN (\q14\q, \q15\q) \n                THEN \q1\q \n            WHEN IFNULL(R.TRADE_CLASSIFY_CODE, SR.TRADE_CLASSIFY_CODE) IS NULL \n                THEN \q1\q \n            WHEN IFNULL(R.TRADE_CLASSIFY_CODE, SR.TRADE_CLASSIFY_CODE) IN (\qSTK-E\q, \qSTK-E2\q, \qSTK-I\q, \qSTK-M1\q, \qSTK-M2\q, \qSTK-T\q) \n                THEN \q1\q \n            ELSE \q0\q \n            END AS REGIST_ACCOUNT_FLAG, \n        STK.WAREHOUSE_CODE, \n        STK.ITEM_CODE, \n        I.ITEM_NAME AS MASTER_ITEM_NAME, \n        I.ITEM_SMALL_CLASS_CODE, \n        STK.ORDER_NO, \n        O.ORDER_DATE, \n        STK.STOCK_CONDITION_CODE, \n        STK.LOT_NO, \n        STK.PACKING_NO, \n        STK.COIL_NO, \n        OI.ITEM_NAME, \n        STK.STOCK_UNIT_CODE, \n        STK.FIRST_RECEIVE_NO, \n        FRE.RECEIVE_DATE AS FIRST_RECEIVE_DATE, \n        IFNULL(R.RECEIVE_DATE, IFNULL(SR.RECEIVE_DATE, IFNULL(STKR.RECEIVE_DATE, STK.IKO_IN_DT))) AS RECEIVE_DATE, \n        IFNULL(IFNULL(RIS.BUY_UNIT_AMOUNT, IFNULL(SRIS.BUY_UNIT_AMOUNT, STKRIS.BUY_UNIT_AMOUNT)), IFNULL(STK.BUY_UNIT_AMOUNT, 0)) AS BUY_UNIT_AMOUNT,\n        IFNULL(\n            RIS.PROFIT_FLAG, \n            IFNULL(\n                SRIS.PROFIT_FLAG, \n                IFNULL(\n                    STKRIS.PROFIT_FLAG, \n                    CASE STK.ASSET_TYPE_CODE\n                        WHEN \q01\q THEN 0\n                        WHEN \q02\q THEN 1\n                        ELSE 0\n                    END\n                )\n            )\n        ) AS RCV_PROFIT_FLAG,\n        \q\q AS PACKING_UNIT_CODE, \n        IFNULL(R.TRADE_CLASSIFY_CODE, IFNULL(SR.TRADE_CLASSIFY_CODE, STKR.TRADE_CLASSIFY_CODE)) AS TRADE_CLASSIFY_CODE, \n        IFNULL(R.SUPPLIER_CODE, IFNULL(SR.SUPPLIER_CODE, IFNULL(STKR.SUPPLIER_CODE, STK.IKO_B_CD))) AS SUPPLIER_CODE, \n        STK.STOCK_STATUS_CODE, \n        STK.STOCK_NOTE, \n        CASE \n            WHEN RID.PROCESS_CLASS_CODE = \q14\q \n                THEN IFNULL(DIVIDE_S.SEND_NO, DIVIDE_SH.SEND_NO) \n            WHEN RID.PROCESS_CLASS_CODE = \q15\q \n                THEN IFNULL(MACHINING_S.SEND_NO, MACHINING_SH.SEND_NO) \n            ELSE NULL \n            END AS DIVIDE_MACHINING_SEND_NO, \n        CASE \n            WHEN RID.PROCESS_CLASS_CODE = \q14\q \n                THEN IFNULL(DIVIDE_S.SELL_RECORD_DATE, DIVIDE_SH.SELL_RECORD_DATE) \n            WHEN RID.PROCESS_CLASS_CODE = \q15\q \n                THEN IFNULL(MACHINING_S.SELL_RECORD_DATE, MACHINING_SH.SELL_RECORD_DATE) \n            ELSE NULL \n            END AS DIVIDE_MACHINING_SELL_RECORD_DATE \n    FROM\n        T_RECEIVE_ITEM_DETAIL RID \n        LEFT OUTER JOIN T_RECEIVE R \n            ON RID.CORP_CODE = R.CORP_CODE \n            AND RID.PROCESS_CLASS_CODE = R.PROCESS_CLASS_CODE \n            AND RID.RECEIVE_NO = R.RECEIVE_NO \n        LEFT OUTER JOIN T_RECEIVE_ITEM_SUMMARY RIS \n            ON RID.CORP_CODE = RIS.CORP_CODE \n            AND RID.PROCESS_CLASS_CODE = RIS.PROCESS_CLASS_CODE \n            AND RID.RECEIVE_NO = RIS.RECEIVE_NO \n            AND RID.DETAIL_NO = RIS.DETAIL_NO \n        LEFT OUTER JOIN T_RECEIVE SR \n            ON RID.CORP_CODE = SR.CORP_CODE \n            AND RID.SRC_PROCESS_CLASS_CODE = SR.PROCESS_CLASS_CODE \n            AND RID.SRC_REPORT_NO = SR.RECEIVE_NO \n        LEFT OUTER JOIN T_RECEIVE_ITEM_SUMMARY SRIS \n            ON RID.CORP_CODE = SRIS.CORP_CODE \n            AND RID.SRC_PROCESS_CLASS_CODE = SRIS.PROCESS_CLASS_CODE \n            AND RID.SRC_REPORT_NO = SRIS.RECEIVE_NO \n            AND RID.SRC_DETAIL_NO = SRIS.DETAIL_NO \n        LEFT OUTER JOIN T_SEND S \n            ON RID.CORP_CODE = S.CORP_CODE \n            AND RID.PROCESS_CLASS_CODE = S.PROCESS_CLASS_CODE \n            AND RID.RECEIVE_NO = S.SEND_NO \n        LEFT OUTER JOIN T_STOCK STK \n            ON RID.CORP_CODE = STK.CORP_CODE \n            AND RID.LATEST_STOCK_NO = STK.STOCK_NO \n            AND RID.LATEST_STOCK_HISTORY_NO = STK.STOCK_HISTORY_NO \n        LEFT OUTER JOIN T_ORDER O \n            ON STK.CORP_CODE = O.CORP_CODE \n            AND STK.ORDER_PROCESS_CLASS_CODE = O.PROCESS_CLASS_CODE \n            AND STK.ORDER_NO = O.ORDER_NO \n        LEFT OUTER JOIN T_ORDER_ITEM OI \n            ON STK.CORP_CODE = OI.CORP_CODE \n            AND STK.ORDER_PROCESS_CLASS_CODE = OI.PROCESS_CLASS_CODE \n            AND STK.ORDER_NO = OI.ORDER_NO \n            AND STK.ORDER_ITEM_DETAIL_NO = OI.DETAIL_NO \n        LEFT OUTER JOIN T_RECEIVE STKR \n            ON STK.CORP_CODE = STKR.CORP_CODE \n            AND STK.RECEIVE_PROCESS_CLASS_CODE = STKR.PROCESS_CLASS_CODE \n            AND STK.RECEIVE_NO = STKR.RECEIVE_NO \n        LEFT OUTER JOIN T_RECEIVE_ITEM_SUMMARY STKRIS \n            ON STK.CORP_CODE = STKRIS.CORP_CODE \n            AND STK.RECEIVE_PROCESS_CLASS_CODE = STKRIS.PROCESS_CLASS_CODE \n            AND STK.RECEIVE_NO = STKRIS.RECEIVE_NO \n            AND STK.RECEIVE_ITEM_SUMMARY_DETAIL_NO = STKRIS.DETAIL_NO \n        LEFT OUTER JOIN M_ITEM I \n            ON STK.CORP_CODE = I.CORP_CODE \n            AND STK.ITEM_CODE = I.ITEM_CODE \n        LEFT OUTER JOIN T_RECEIVE FRE \n            ON STK.CORP_CODE = FRE.CORP_CODE \n            AND STK.FIRST_RECEIVE_NO = FRE.RECEIVE_NO \n        LEFT OUTER JOIN T_SEND_ITEM_DETAIL DIVIDE_SID \n            ON STKRIS.CORP_CODE = DIVIDE_SID.CORP_CODE \n            AND STKRIS.ORI_STOCK_NO = DIVIDE_SID.LATEST_STOCK_NO \n            AND STKRIS.ORI_STOCK_HISTORY_NO = DIVIDE_SID.LATEST_STOCK_HISTORY_NO \n            AND STKRIS.PROCESS_CLASS_CODE = \q14\q \n        LEFT OUTER JOIN T_SEND DIVIDE_S \n            ON DIVIDE_SID.CORP_CODE = DIVIDE_S.CORP_CODE \n            AND DIVIDE_SID.PROCESS_CLASS_CODE = DIVIDE_S.PROCESS_CLASS_CODE \n            AND DIVIDE_SID.REPORT_NO = DIVIDE_S.SEND_NO \n            AND DIVIDE_S.REVISION_NO = 0 \n        LEFT OUTER JOIN T_SEND_HISTORY DIVIDE_SH \n            ON DIVIDE_SID.CORP_CODE = DIVIDE_SH.CORP_CODE \n            AND DIVIDE_SID.PROCESS_CLASS_CODE = DIVIDE_SH.PROCESS_CLASS_CODE \n            AND DIVIDE_SID.REPORT_NO = DIVIDE_SH.SEND_NO \n            AND DIVIDE_SH.REVISION_NO = 0 \n            AND DIVIDE_SH.RB_TYPE = \q0\q \n        LEFT OUTER JOIN T_SEND MACHINING_S \n            ON R.CORP_CODE = MACHINING_S.CORP_CODE \n            AND R.MACHINING_SEND_PROCESS_CLASS_CODE = MACHINING_S.PROCESS_CLASS_CODE \n            AND R.MACHINING_SEND_NO = MACHINING_S.SEND_NO \n            AND R.PROCESS_CLASS_CODE = \q15\q \n            AND MACHINING_S.REVISION_NO = 0 \n        LEFT OUTER JOIN T_SEND_HISTORY MACHINING_SH \n            ON R.CORP_CODE = MACHINING_SH.CORP_CODE \n            AND R.MACHINING_SEND_PROCESS_CLASS_CODE = MACHINING_SH.PROCESS_CLASS_CODE \n            AND R.MACHINING_SEND_NO = MACHINING_SH.SEND_NO \n            AND R.PROCESS_CLASS_CODE = \q15\q \n            AND MACHINING_SH.REVISION_NO = 0 \n            AND MACHINING_SH.RB_TYPE = \q0\q \n    WHERE\n        IFNULL(R.RB_TYPE, IFNULL(SR.RB_TYPE, \q0\q)) = \q0\q \n        AND RID.PROCESS_CLASS_CODE NOT IN (\q38\q, \q41\q)\n) \nUNION ALL ( \n    SELECT\n        RIDH.CORP_CODE, \n        RIDH.PROCESS_CLASS_CODE, \n        RIDH.RECEIVE_NO, \n        RIDH.REVISION_NO, \n        RIDH.DETAIL_NO, \n        RIDH.STOCK_NO, \n        RIDH.STOCK_HISTORY_NO, \n        RIDH.SRC_PROCESS_CLASS_CODE, \n        RIDH.SRC_REPORT_NO, \n        RIDH.SRC_DETAIL_NO, \n        RIDH.LATEST_STOCK_NO, \n        RIDH.LATEST_STOCK_HISTORY_NO, \n        RIDH.SRC_STOCK_NO, \n        RIDH.SRC_STOCK_HISTORY_NO, \n        RIDH.BASE_COST_AMOUNT * - 1 AS BASE_COST_AMOUNT, \n        RIDH.BASE_COST_ADD_AMOUNT * - 1 AS BASE_COST_ADD_AMOUNT, \n        RIDH.BASE_COST_CHARGE_AMOUNT * - 1 AS BASE_COST_CHARGE_AMOUNT, \n        RIDH.CREATE_DATETIME, \n        RIDH.CREATE_USER_CODE, \n        RIDH.UPDATE_DATETIME, \n        RIDH.UPDATE_USER_CODE, \n        STK.STOCK_ID, \n        STK.IKO_IN_DT, \n        STK.ASSET_TYPE_CODE AS STK_ASSET_TYPE_CODE, \n        STK.STOCK_QUANTITY * - 1 AS STOCK_QUANTITY, \n        IFNULL( \n            RH.PROCESS_CLASS_CODE, \n            IFNULL(SRH.PROCESS_CLASS_CODE, IFNULL(R.PROCESS_CLASS_CODE, SR.PROCESS_CLASS_CODE))\n        ) AS RCV_PROCESS_CLASS_CODE, \n        IFNULL(RH.RECEIVE_NO, IFNULL(SRH.RECEIVE_NO, IFNULL(R.RECEIVE_NO, SR.RECEIVE_NO))) AS RCV_RECEIVE_NO, \n        RIDH.REVISION_NO AS RCV_REVISION_NO, \n        IFNULL(RISH.DETAIL_NO, IFNULL(SRISH.DETAIL_NO, IFNULL(RIS.DETAIL_NO, SRIS.DETAIL_NO))) AS RCV_DETAIL_NO, \n        IFNULL(RH.RB_TYPE, IFNULL(SH.RB_TYPE, IFNULL(R.RB_TYPE, SR.RB_TYPE))) AS RB_TYPE, \n        IFNULL(RH.ASSET_TYPE_CODE, IFNULL(SRH.ASSET_TYPE_CODE, IFNULL(R.ASSET_TYPE_CODE, SR.ASSET_TYPE_CODE))) AS RCV_ASSET_TYPE_CODE, \n        IFNULL(RH.BUY_RECORD_DATE, IFNULL(SRH.BUY_RECORD_DATE, IFNULL(R.BUY_RECORD_DATE, SR.BUY_RECORD_DATE))) AS BUY_RECORD_DATE, \n        IFNULL(RH.CREATE_DATETIME, IFNULL(SRH.CREATE_DATETIME, IFNULL(R.CREATE_DATETIME, SR.CREATE_DATETIME))) AS RCV_CREATE_DATETIME, \n        CASE \n            WHEN RIDH.PROCESS_CLASS_CODE IN (\q14\q, \q15\q) \n                THEN \q1\q \n            WHEN IFNULL( \n                RH.TRADE_CLASSIFY_CODE, \n                IFNULL(SRH.TRADE_CLASSIFY_CODE, IFNULL(R.TRADE_CLASSIFY_CODE, SR.TRADE_CLASSIFY_CODE))\n            ) IS NULL \n                THEN \q1\q \n            WHEN IFNULL( \n                RH.TRADE_CLASSIFY_CODE, \n                IFNULL(SRH.TRADE_CLASSIFY_CODE, IFNULL(R.TRADE_CLASSIFY_CODE, SR.TRADE_CLASSIFY_CODE))\n            ) IN (\qSTK-E\q, \qSTK-E2\q, \qSTK-I\q, \qSTK-M1\q, \qSTK-M2\q, \qSTK-T\q) \n                THEN \q1\q \n            ELSE \q0\q \n            END AS REGIST_ACCOUNT_FLAG, \n        STK.WAREHOUSE_CODE, \n        STK.ITEM_CODE, \n        I.ITEM_NAME AS MASTER_ITEM_NAME, \n        I.ITEM_SMALL_CLASS_CODE, \n        STK.ORDER_NO, \n        O.ORDER_DATE, \n        STK.STOCK_CONDITION_CODE, \n        STK.LOT_NO, \n        STK.PACKING_NO, \n        STK.COIL_NO, \n        OI.ITEM_NAME, \n        STK.STOCK_UNIT_CODE, \n        STK.FIRST_RECEIVE_NO, \n        FRE.RECEIVE_DATE AS FIRST_RECEIVE_DATE, \n        IFNULL(RH.RECEIVE_DATE, IFNULL(SRH.RECEIVE_DATE, IFNULL(R.RECEIVE_DATE, SR.RECEIVE_DATE))) AS RECEIVE_DATE, \n        IFNULL( \n            RISH.BUY_UNIT_AMOUNT, \n            IFNULL(SRISH.BUY_UNIT_AMOUNT, IFNULL(RIS.BUY_UNIT_AMOUNT, IFNULL(SRIS.BUY_UNIT_AMOUNT, IFNULL(STK.BUY_UNIT_AMOUNT, 0))))\n        ) AS BUY_UNIT_AMOUNT, \n        IFNULL( \n            RISH.PROFIT_FLAG, \n            IFNULL(\n                SRISH.PROFIT_FLAG, \n                IFNULL(\n                    RIS.PROFIT_FLAG, \n                    IFNULL(\n                        SRIS.PROFIT_FLAG,\n                        CASE STK.ASSET_TYPE_CODE\n                            WHEN \q01\q THEN 0\n                            WHEN \q02\q THEN 1\n                            ELSE 0\n                        END\n                    )\n                )\n            )\n        ) AS RCV_PROFIT_FLAG, \n        \q\q AS PACKING_UNIT_CODE, \n        IFNULL( \n            RH.TRADE_CLASSIFY_CODE, \n            IFNULL(SRH.TRADE_CLASSIFY_CODE, IFNULL(R.TRADE_CLASSIFY_CODE, SR.TRADE_CLASSIFY_CODE))\n        ) AS TRADE_CLASSIFY_CODE, \n        IFNULL(RH.SUPPLIER_CODE, IFNULL(SRH.SUPPLIER_CODE, IFNULL(R.SUPPLIER_CODE, SR.SUPPLIER_CODE))) AS SUPPLIER_CODE, \n        STK.STOCK_STATUS_CODE, \n        STK.STOCK_NOTE, \n        CASE \n            WHEN RIDH.PROCESS_CLASS_CODE = \q14\q \n                THEN IFNULL(DIVIDE_S.SEND_NO, DIVIDE_SH.SEND_NO) \n            WHEN RIDH.PROCESS_CLASS_CODE = \q15\q \n                THEN IFNULL(MACHINING_S.SEND_NO, MACHINING_SH.SEND_NO) \n            ELSE NULL \n            END AS DIVIDE_MACHINING_SEND_NO, \n        CASE \n            WHEN RIDH.PROCESS_CLASS_CODE = \q14\q \n                THEN IFNULL(DIVIDE_S.SELL_RECORD_DATE, DIVIDE_SH.SELL_RECORD_DATE) \n            WHEN RIDH.PROCESS_CLASS_CODE = \q15\q \n                THEN IFNULL(MACHINING_S.SELL_RECORD_DATE, MACHINING_SH.SELL_RECORD_DATE) \n            ELSE NULL \n            END AS DIVIDE_MACHINING_SELL_RECORD_DATE \n    FROM\n        T_RECEIVE_ITEM_DETAIL_HISTORY RIDH \n        LEFT OUTER JOIN T_RECEIVE_HISTORY RH \n            ON RIDH.CORP_CODE = RH.CORP_CODE \n            AND RIDH.PROCESS_CLASS_CODE = RH.PROCESS_CLASS_CODE \n            AND RIDH.RECEIVE_NO = RH.RECEIVE_NO \n            AND RIDH.REVISION_NO = RH.REVISION_NO \n            AND RH.RB_TYPE = \q1\q \n        LEFT OUTER JOIN T_RECEIVE_ITEM_SUMMARY_HISTORY RISH \n            ON RIDH.CORP_CODE = RISH.CORP_CODE \n            AND RIDH.PROCESS_CLASS_CODE = RISH.PROCESS_CLASS_CODE \n            AND RIDH.RECEIVE_NO = RISH.RECEIVE_NO \n            AND RIDH.REVISION_NO = RISH.REVISION_NO \n            AND RIDH.DETAIL_NO = RISH.DETAIL_NO \n            AND RH.RB_TYPE = \q1\q \n        LEFT OUTER JOIN T_RECEIVE_HISTORY SRH \n            ON RIDH.CORP_CODE = SRH.CORP_CODE \n            AND RIDH.SRC_PROCESS_CLASS_CODE = SRH.PROCESS_CLASS_CODE \n            AND RIDH.SRC_REPORT_NO = SRH.RECEIVE_NO \n            AND RIDH.REVISION_NO = SRH.REVISION_NO \n            AND SRH.RB_TYPE = \q1\q \n        LEFT OUTER JOIN T_RECEIVE_ITEM_SUMMARY_HISTORY SRISH \n            ON RIDH.CORP_CODE = SRISH.CORP_CODE \n            AND RIDH.SRC_PROCESS_CLASS_CODE = SRISH.PROCESS_CLASS_CODE \n            AND RIDH.SRC_REPORT_NO = SRISH.RECEIVE_NO \n            AND RIDH.REVISION_NO = SRISH.REVISION_NO \n            AND RIDH.SRC_DETAIL_NO = SRISH.DETAIL_NO \n            AND SRH.RB_TYPE = \q1\q \n        LEFT OUTER JOIN T_RECEIVE R \n            ON RIDH.CORP_CODE = R.CORP_CODE \n            AND RIDH.PROCESS_CLASS_CODE = R.PROCESS_CLASS_CODE \n            AND RIDH.RECEIVE_NO = R.RECEIVE_NO \n        LEFT OUTER JOIN T_RECEIVE_ITEM_SUMMARY RIS \n            ON RIDH.CORP_CODE = RIS.CORP_CODE \n            AND RIDH.PROCESS_CLASS_CODE = RIS.PROCESS_CLASS_CODE \n            AND RIDH.RECEIVE_NO = RIS.RECEIVE_NO \n            AND RIDH.DETAIL_NO = RIS.DETAIL_NO \n        LEFT OUTER JOIN T_RECEIVE SR \n            ON RIDH.CORP_CODE = SR.CORP_CODE \n            AND RIDH.SRC_PROCESS_CLASS_CODE = SR.PROCESS_CLASS_CODE \n            AND RIDH.SRC_REPORT_NO = SR.RECEIVE_NO \n        LEFT OUTER JOIN T_RECEIVE_ITEM_SUMMARY SRIS \n            ON RIDH.CORP_CODE = SRIS.CORP_CODE \n            AND RIDH.SRC_PROCESS_CLASS_CODE = SRIS.PROCESS_CLASS_CODE \n            AND RIDH.SRC_REPORT_NO = SRIS.RECEIVE_NO \n            AND RIDH.SRC_DETAIL_NO = SRIS.DETAIL_NO \n        LEFT OUTER JOIN T_SEND_HISTORY SH \n            ON RIDH.CORP_CODE = SH.CORP_CODE \n            AND RIDH.PROCESS_CLASS_CODE = SH.PROCESS_CLASS_CODE \n            AND RIDH.RECEIVE_NO = SH.SEND_NO \n            AND RIDH.REVISION_NO = SH.REVISION_NO \n            AND SH.RB_TYPE = \q1\q \n        LEFT OUTER JOIN T_STOCK STK \n            ON RIDH.CORP_CODE = STK.CORP_CODE \n            AND RIDH.LATEST_STOCK_NO = STK.STOCK_NO \n            AND RIDH.LATEST_STOCK_HISTORY_NO = STK.STOCK_HISTORY_NO \n        LEFT OUTER JOIN T_ORDER O \n            ON STK.CORP_CODE = O.CORP_CODE \n            AND STK.ORDER_PROCESS_CLASS_CODE = O.PROCESS_CLASS_CODE \n            AND STK.ORDER_NO = O.ORDER_NO \n        LEFT OUTER JOIN T_ORDER_ITEM OI \n            ON STK.CORP_CODE = OI.CORP_CODE \n            AND STK.ORDER_PROCESS_CLASS_CODE = OI.PROCESS_CLASS_CODE \n            AND STK.ORDER_NO = OI.ORDER_NO \n            AND STK.ORDER_ITEM_DETAIL_NO = OI.DETAIL_NO \n        LEFT OUTER JOIN M_ITEM I \n            ON STK.CORP_CODE = I.CORP_CODE \n            AND STK.ITEM_CODE = I.ITEM_CODE \n        LEFT OUTER JOIN T_RECEIVE FRE \n            ON STK.CORP_CODE = FRE.CORP_CODE \n            AND STK.FIRST_RECEIVE_NO = FRE.RECEIVE_NO \n        LEFT OUTER JOIN T_RECEIVE_ITEM_SUMMARY STKRIS \n            ON STK.CORP_CODE = STKRIS.CORP_CODE \n            AND STK.RECEIVE_PROCESS_CLASS_CODE = STKRIS.PROCESS_CLASS_CODE \n            AND STK.RECEIVE_NO = STKRIS.RECEIVE_NO \n            AND STK.RECEIVE_ITEM_SUMMARY_DETAIL_NO = STKRIS.DETAIL_NO \n        LEFT OUTER JOIN T_SEND_ITEM_DETAIL DIVIDE_SID \n            ON STKRIS.CORP_CODE = DIVIDE_SID.CORP_CODE \n            AND STKRIS.ORI_STOCK_NO = DIVIDE_SID.LATEST_STOCK_NO \n            AND STKRIS.ORI_STOCK_HISTORY_NO = DIVIDE_SID.LATEST_STOCK_HISTORY_NO \n            AND STKRIS.PROCESS_CLASS_CODE = \q14\q \n        LEFT OUTER JOIN T_SEND DIVIDE_S \n            ON DIVIDE_SID.CORP_CODE = DIVIDE_S.CORP_CODE \n            AND DIVIDE_SID.PROCESS_CLASS_CODE = DIVIDE_S.PROCESS_CLASS_CODE \n            AND DIVIDE_SID.REPORT_NO = DIVIDE_S.SEND_NO \n            AND DIVIDE_S.REVISION_NO = 0 \n        LEFT OUTER JOIN T_SEND_HISTORY DIVIDE_SH \n            ON DIVIDE_SID.CORP_CODE = DIVIDE_SH.CORP_CODE \n            AND DIVIDE_SID.PROCESS_CLASS_CODE = DIVIDE_SH.PROCESS_CLASS_CODE \n            AND DIVIDE_SID.REPORT_NO = DIVIDE_SH.SEND_NO \n            AND DIVIDE_SH.REVISION_NO = 0 \n            AND DIVIDE_SH.RB_TYPE = \q0\q \n        LEFT OUTER JOIN T_SEND MACHINING_S \n            ON R.CORP_CODE = MACHINING_S.CORP_CODE \n            AND R.MACHINING_SEND_PROCESS_CLASS_CODE = MACHINING_S.PROCESS_CLASS_CODE \n            AND R.MACHINING_SEND_NO = MACHINING_S.SEND_NO \n            AND R.PROCESS_CLASS_CODE = \q15\q \n            AND MACHINING_S.REVISION_NO = 0 \n        LEFT OUTER JOIN T_SEND_HISTORY MACHINING_SH \n            ON R.CORP_CODE = MACHINING_SH.CORP_CODE \n            AND R.MACHINING_SEND_PROCESS_CLASS_CODE = MACHINING_SH.PROCESS_CLASS_CODE \n            AND R.MACHINING_SEND_NO = MACHINING_SH.SEND_NO \n            AND R.PROCESS_CLASS_CODE = \q15\q \n            AND MACHINING_SH.REVISION_NO = 0 \n            AND MACHINING_SH.RB_TYPE = \q0\q \n    WHERE\n        IFNULL(RH.RB_TYPE, IFNULL(SRH.RB_TYPE, SH.RB_TYPE)) = \q1\q\n) \nUNION ALL ( \n    SELECT\n        RIDH.CORP_CODE, \n        RIDH.PROCESS_CLASS_CODE, \n        RIDH.RECEIVE_NO, \n        RIDH.REVISION_NO, \n        RIDH.DETAIL_NO, \n        RIDH.STOCK_NO, \n        RIDH.STOCK_HISTORY_NO, \n        RIDH.SRC_PROCESS_CLASS_CODE, \n        RIDH.SRC_REPORT_NO, \n        RIDH.SRC_DETAIL_NO, \n        RIDH.LATEST_STOCK_NO, \n        RIDH.LATEST_STOCK_HISTORY_NO, \n        RIDH.SRC_STOCK_NO, \n        RIDH.SRC_STOCK_HISTORY_NO, \n        RIDH.BASE_COST_AMOUNT, \n        RIDH.BASE_COST_ADD_AMOUNT, \n        RIDH.BASE_COST_CHARGE_AMOUNT, \n        RIDH.CREATE_DATETIME, \n        RIDH.CREATE_USER_CODE, \n        RIDH.UPDATE_DATETIME, \n        RIDH.UPDATE_USER_CODE, \n        STK.STOCK_ID, \n        STK.IKO_IN_DT, \n        STK.ASSET_TYPE_CODE AS STK_ASSET_TYPE_CODE, \n        STK.STOCK_QUANTITY, \n        IFNULL(RH.PROCESS_CLASS_CODE, IFNULL(SRH.PROCESS_CLASS_CODE, SR.PROCESS_CLASS_CODE)) AS RCV_PROCESS_CLASS_CODE, \n        IFNULL(RH.RECEIVE_NO, IFNULL(SRH.RECEIVE_NO, SR.RECEIVE_NO)) AS RCV_RECEIVE_NO, \n        RIDH.REVISION_NO AS RCV_REVISION_NO, \n        IFNULL(RISH.DETAIL_NO, IFNULL(SRISH.DETAIL_NO, SRIS.DETAIL_NO)) AS RCV_DETAIL_NO, \n        IFNULL(RH.RB_TYPE, IFNULL(SRH.RB_TYPE, IFNULL(SR.RB_TYPE, SH.RB_TYPE))) AS RB_TYPE, \n        IFNULL(RH.ASSET_TYPE_CODE, IFNULL(SRH.ASSET_TYPE_CODE, SR.ASSET_TYPE_CODE)) AS RCV_ASSET_TYPE_CODE, \n        IFNULL(RH.BUY_RECORD_DATE, IFNULL(SRH.BUY_RECORD_DATE, SR.BUY_RECORD_DATE)) AS BUY_RECORD_DATE, \n        IFNULL(RH.CREATE_DATETIME, IFNULL(SRH.CREATE_DATETIME, SR.CREATE_DATETIME)) AS RCV_CREATE_DATETIME, \n        CASE \n            WHEN RIDH.PROCESS_CLASS_CODE IN (\q14\q, \q15\q) \n                THEN \q1\q \n            WHEN IFNULL(RH.TRADE_CLASSIFY_CODE, SRH.TRADE_CLASSIFY_CODE) IS NULL \n                THEN \q1\q \n            WHEN IFNULL(RH.TRADE_CLASSIFY_CODE, SRH.TRADE_CLASSIFY_CODE) IN (\qSTK-E\q, \qSTK-E2\q, \qSTK-I\q, \qSTK-M1\q, \qSTK-M2\q, \qSTK-T\q) \n                THEN \q1\q \n            ELSE \q0\q \n            END AS REGIST_ACCOUNT_FLAG, \n        STK.WAREHOUSE_CODE, \n        STK.ITEM_CODE, \n        I.ITEM_NAME AS MASTER_ITEM_NAME, \n        I.ITEM_SMALL_CLASS_CODE, \n        STK.ORDER_NO, \n        O.ORDER_DATE, \n        STK.STOCK_CONDITION_CODE, \n        STK.LOT_NO, \n        STK.PACKING_NO, \n        STK.COIL_NO, \n        OI.ITEM_NAME, \n        STK.STOCK_UNIT_CODE, \n        STK.FIRST_RECEIVE_NO, \n        FRE.RECEIVE_DATE AS FIRST_RECEIVE_DATE, \n        IFNULL(RH.RECEIVE_DATE, IFNULL(SRH.RECEIVE_DATE, SR.RECEIVE_DATE)) AS RECEIVE_DATE, \n        IFNULL(IFNULL(RISH.BUY_UNIT_AMOUNT, IFNULL(SRISH.BUY_UNIT_AMOUNT, SRIS.BUY_UNIT_AMOUNT)), IFNULL(STK.BUY_UNIT_AMOUNT, 0)) AS BUY_UNIT_AMOUNT, \n        IFNULL(\n            RISH.PROFIT_FLAG, \n            IFNULL(\n                SRISH.PROFIT_FLAG, \n                IFNULL(\n                    SRISH.PROFIT_FLAG,\n                    CASE STK.ASSET_TYPE_CODE\n                        WHEN \q01\q THEN 0\n                        WHEN \q02\q THEN 1\n                        ELSE 0\n                    END\n                )\n            )\n        ) AS RCV_PROFIT_FLAG, \n        \q\q AS PACKING_UNIT_CODE, \n        IFNULL(RH.TRADE_CLASSIFY_CODE, IFNULL(SRH.TRADE_CLASSIFY_CODE, SR.TRADE_CLASSIFY_CODE)) AS TRADE_CLASSIFY_CODE, \n        IFNULL(RH.SUPPLIER_CODE, IFNULL(SRH.SUPPLIER_CODE, SR.SUPPLIER_CODE)) AS SUPPLIER_CODE, \n        STK.STOCK_STATUS_CODE, \n        STK.STOCK_NOTE, \n        CASE \n            WHEN RIDH.PROCESS_CLASS_CODE = \q14\q \n                THEN IFNULL(DIVIDE_S.SEND_NO, DIVIDE_SH.SEND_NO) \n            WHEN RIDH.PROCESS_CLASS_CODE = \q15\q \n                THEN IFNULL(MACHINING_S.SEND_NO, MACHINING_SH.SEND_NO) \n            ELSE NULL \n            END AS DIVIDE_MACHINING_SEND_NO, \n        CASE \n            WHEN RIDH.PROCESS_CLASS_CODE = \q14\q \n                THEN IFNULL(DIVIDE_S.SELL_RECORD_DATE, DIVIDE_SH.SELL_RECORD_DATE) \n            WHEN RIDH.PROCESS_CLASS_CODE = \q15\q \n                THEN IFNULL(MACHINING_S.SELL_RECORD_DATE, MACHINING_SH.SELL_RECORD_DATE) \n            ELSE NULL \n            END AS DIVIDE_MACHINING_SELL_RECORD_DATE \n    FROM\n        T_RECEIVE_ITEM_DETAIL_HISTORY RIDH \n        LEFT OUTER JOIN T_RECEIVE_HISTORY RH \n            ON RIDH.CORP_CODE = RH.CORP_CODE \n            AND RIDH.PROCESS_CLASS_CODE = RH.PROCESS_CLASS_CODE \n            AND RIDH.RECEIVE_NO = RH.RECEIVE_NO \n            AND RIDH.REVISION_NO = RH.REVISION_NO \n            AND RH.RB_TYPE = \q0\q \n        LEFT OUTER JOIN T_RECEIVE_ITEM_SUMMARY_HISTORY RISH \n            ON RIDH.CORP_CODE = RISH.CORP_CODE \n            AND RIDH.PROCESS_CLASS_CODE = RISH.PROCESS_CLASS_CODE \n            AND RIDH.RECEIVE_NO = RISH.RECEIVE_NO \n            AND RIDH.REVISION_NO = RISH.REVISION_NO \n            AND RIDH.DETAIL_NO = RISH.DETAIL_NO \n            AND RH.RB_TYPE = \q0\q \n        LEFT OUTER JOIN T_RECEIVE SR \n            ON RIDH.CORP_CODE = SR.CORP_CODE \n            AND RIDH.SRC_PROCESS_CLASS_CODE = SR.PROCESS_CLASS_CODE \n            AND RIDH.SRC_REPORT_NO = SR.RECEIVE_NO \n            AND SR.RB_TYPE = \q0\q \n            AND RIDH.RECEIVE_NO != RIDH.SRC_REPORT_NO \n        LEFT OUTER JOIN T_RECEIVE_ITEM_SUMMARY SRIS \n            ON RIDH.CORP_CODE = SRIS.CORP_CODE \n            AND RIDH.SRC_PROCESS_CLASS_CODE = SRIS.PROCESS_CLASS_CODE \n            AND RIDH.SRC_REPORT_NO = SRIS.RECEIVE_NO \n            AND RIDH.SRC_DETAIL_NO = SRIS.DETAIL_NO \n            AND SR.RB_TYPE = \q0\q \n            AND RIDH.RECEIVE_NO != RIDH.SRC_REPORT_NO \n        LEFT OUTER JOIN T_RECEIVE_HISTORY SRH \n            ON RIDH.CORP_CODE = SRH.CORP_CODE \n            AND RIDH.SRC_PROCESS_CLASS_CODE = SRH.PROCESS_CLASS_CODE \n            AND RIDH.SRC_REPORT_NO = SRH.RECEIVE_NO \n            AND RIDH.REVISION_NO = SRH.REVISION_NO \n            AND SRH.RB_TYPE = \q0\q \n            AND RIDH.RECEIVE_NO != RIDH.SRC_REPORT_NO \n        LEFT OUTER JOIN T_RECEIVE_ITEM_SUMMARY_HISTORY SRISH \n            ON RIDH.CORP_CODE = SRISH.CORP_CODE \n            AND RIDH.SRC_PROCESS_CLASS_CODE = SRISH.PROCESS_CLASS_CODE \n            AND RIDH.SRC_REPORT_NO = SRISH.RECEIVE_NO \n            AND RIDH.REVISION_NO = SRISH.REVISION_NO \n            AND RIDH.SRC_DETAIL_NO = SRISH.DETAIL_NO \n            AND SRH.RB_TYPE = \q0\q \n            AND RIDH.RECEIVE_NO != RIDH.SRC_REPORT_NO \n        LEFT OUTER JOIN T_SEND_HISTORY SH \n            ON RIDH.CORP_CODE = SH.CORP_CODE \n            AND RIDH.PROCESS_CLASS_CODE = SH.PROCESS_CLASS_CODE \n            AND RIDH.RECEIVE_NO = SH.SEND_NO \n            AND RIDH.REVISION_NO = SH.REVISION_NO \n            AND SH.RB_TYPE = \q0\q \n        LEFT OUTER JOIN T_STOCK STK \n            ON RIDH.CORP_CODE = STK.CORP_CODE \n            AND RIDH.LATEST_STOCK_NO = STK.STOCK_NO \n            AND RIDH.LATEST_STOCK_HISTORY_NO = STK.STOCK_HISTORY_NO \n        LEFT OUTER JOIN T_ORDER O \n            ON STK.CORP_CODE = O.CORP_CODE \n            AND STK.ORDER_PROCESS_CLASS_CODE = O.PROCESS_CLASS_CODE \n            AND STK.ORDER_NO = O.ORDER_NO \n        LEFT OUTER JOIN T_ORDER_ITEM OI \n            ON STK.CORP_CODE = OI.CORP_CODE \n            AND STK.ORDER_PROCESS_CLASS_CODE = OI.PROCESS_CLASS_CODE \n            AND STK.ORDER_NO = OI.ORDER_NO \n            AND STK.ORDER_ITEM_DETAIL_NO = OI.DETAIL_NO \n        LEFT OUTER JOIN M_ITEM I \n            ON STK.CORP_CODE = I.CORP_CODE \n            AND STK.ITEM_CODE = I.ITEM_CODE \n        LEFT OUTER JOIN T_RECEIVE FRE \n            ON STK.CORP_CODE = FRE.CORP_CODE \n            AND STK.FIRST_RECEIVE_NO = FRE.RECEIVE_NO \n        LEFT OUTER JOIN T_RECEIVE_ITEM_SUMMARY STKRIS \n            ON STK.CORP_CODE = STKRIS.CORP_CODE \n            AND STK.RECEIVE_PROCESS_CLASS_CODE = STKRIS.PROCESS_CLASS_CODE \n            AND STK.RECEIVE_NO = STKRIS.RECEIVE_NO \n            AND STK.RECEIVE_ITEM_SUMMARY_DETAIL_NO = STKRIS.DETAIL_NO \n        LEFT OUTER JOIN T_SEND_ITEM_DETAIL DIVIDE_SID \n            ON STKRIS.CORP_CODE = DIVIDE_SID.CORP_CODE \n            AND STKRIS.ORI_STOCK_NO = DIVIDE_SID.LATEST_STOCK_NO \n            AND STKRIS.ORI_STOCK_HISTORY_NO = DIVIDE_SID.LATEST_STOCK_HISTORY_NO \n            AND STKRIS.PROCESS_CLASS_CODE = \q14\q \n        LEFT OUTER JOIN T_SEND DIVIDE_S \n            ON DIVIDE_SID.CORP_CODE = DIVIDE_S.CORP_CODE \n            AND DIVIDE_SID.PROCESS_CLASS_CODE = DIVIDE_S.PROCESS_CLASS_CODE \n            AND DIVIDE_SID.REPORT_NO = DIVIDE_S.SEND_NO \n            AND DIVIDE_S.REVISION_NO = 0 \n        LEFT OUTER JOIN T_SEND_HISTORY DIVIDE_SH \n            ON DIVIDE_SID.CORP_CODE = DIVIDE_SH.CORP_CODE \n            AND DIVIDE_SID.PROCESS_CLASS_CODE = DIVIDE_SH.PROCESS_CLASS_CODE \n            AND DIVIDE_SID.REPORT_NO = DIVIDE_SH.SEND_NO \n            AND DIVIDE_SH.REVISION_NO = 0 \n            AND DIVIDE_SH.RB_TYPE = \q0\q \n        LEFT OUTER JOIN T_SEND MACHINING_S \n            ON RH.CORP_CODE = MACHINING_S.CORP_CODE \n            AND RH.MACHINING_SEND_PROCESS_CLASS_CODE = MACHINING_S.PROCESS_CLASS_CODE \n            AND RH.MACHINING_SEND_NO = MACHINING_S.SEND_NO \n            AND RH.PROCESS_CLASS_CODE = \q15\q \n            AND MACHINING_S.REVISION_NO = 0 \n        LEFT OUTER JOIN T_SEND_HISTORY MACHINING_SH \n            ON RH.CORP_CODE = MACHINING_SH.CORP_CODE \n            AND RH.MACHINING_SEND_PROCESS_CLASS_CODE = MACHINING_SH.PROCESS_CLASS_CODE \n            AND RH.MACHINING_SEND_NO = MACHINING_SH.SEND_NO \n            AND RH.PROCESS_CLASS_CODE = \q15\q \n            AND MACHINING_SH.REVISION_NO = 0 \n            AND MACHINING_SH.RB_TYPE = \q0\q \n    WHERE\n        IFNULL(RH.RB_TYPE, IFNULL(SRH.RB_TYPE, IFNULL(SR.RB_TYPE, SH.RB_TYPE))) = \q0\q\n) \nUNION ALL ( \n    SELECT\n        RIDH.CORP_CODE, \n        RIDH.PROCESS_CLASS_CODE, \n        RIDH.RECEIVE_NO, \n        RIDH.REVISION_NO, \n        RIDH.DETAIL_NO, \n        RIDH.STOCK_NO, \n        RIDH.STOCK_HISTORY_NO, \n        RIDH.SRC_PROCESS_CLASS_CODE, \n        RIDH.SRC_REPORT_NO, \n        RIDH.SRC_DETAIL_NO, \n        RIDH.LATEST_STOCK_NO, \n        RIDH.LATEST_STOCK_HISTORY_NO, \n        RIDH.SRC_STOCK_NO, \n        RIDH.SRC_STOCK_HISTORY_NO, \n        RIDH.BASE_COST_AMOUNT * - 1 AS BASE_COST_AMOUNT, \n        RIDH.BASE_COST_ADD_AMOUNT * - 1 AS BASE_COST_ADD_AMOUNT, \n        RIDH.BASE_COST_CHARGE_AMOUNT * - 1 AS BASE_COST_CHARGE_AMOUNT, \n        RIDH.CREATE_DATETIME, \n        RIDH.CREATE_USER_CODE, \n        RIDH.UPDATE_DATETIME, \n        RIDH.UPDATE_USER_CODE, \n        STK.STOCK_ID, \n        STK.IKO_IN_DT, \n        STK.ASSET_TYPE_CODE AS STK_ASSET_TYPE_CODE, \n        STK.STOCK_QUANTITY * - 1 AS STOCK_QUANTITY, \n        IFNULL(R.PROCESS_CLASS_CODE, SR.PROCESS_CLASS_CODE) AS RCV_PROCESS_CLASS_CODE, \n        IFNULL(R.RECEIVE_NO, SR.RECEIVE_NO) AS RCV_RECEIVE_NO, \n        RIDH.REVISION_NO AS RCV_REVISION_NO, \n        IFNULL(RIS.DETAIL_NO, SRIS.DETAIL_NO) AS RCV_DETAIL_NO, \n        IFNULL(R.RB_TYPE, S.RB_TYPE) AS RB_TYPE, \n        IFNULL(R.ASSET_TYPE_CODE, SR.ASSET_TYPE_CODE) AS RCV_ASSET_TYPE_CODE, \n        IFNULL(R.BUY_RECORD_DATE, SR.BUY_RECORD_DATE) AS BUY_RECORD_DATE, \n        IFNULL(R.CREATE_DATETIME, SR.CREATE_DATETIME) AS RCV_CREATE_DATETIME, \n        CASE \n            WHEN RIDH.PROCESS_CLASS_CODE IN (\q14\q, \q15\q) \n                THEN \q1\q \n            WHEN IFNULL(R.TRADE_CLASSIFY_CODE, SR.TRADE_CLASSIFY_CODE) IS NULL \n                THEN \q1\q \n            WHEN IFNULL(R.TRADE_CLASSIFY_CODE, SR.TRADE_CLASSIFY_CODE) IN (\qSTK-E\q, \qSTK-E2\q, \qSTK-I\q, \qSTK-M1\q, \qSTK-M2\q, \qSTK-T\q) \n                THEN \q1\q \n            ELSE \q0\q \n            END AS REGIST_ACCOUNT_FLAG, \n        STK.WAREHOUSE_CODE, \n        STK.ITEM_CODE, \n        I.ITEM_NAME AS MASTER_ITEM_NAME, \n        I.ITEM_SMALL_CLASS_CODE, \n        STK.ORDER_NO, \n        O.ORDER_DATE, \n        STK.STOCK_CONDITION_CODE, \n        STK.LOT_NO, \n        STK.PACKING_NO, \n        STK.COIL_NO, \n        OI.ITEM_NAME, \n        STK.STOCK_UNIT_CODE, \n        STK.FIRST_RECEIVE_NO, \n        FRE.RECEIVE_DATE AS FIRST_RECEIVE_DATE, \n        IFNULL(R.RECEIVE_DATE, SR.RECEIVE_DATE) AS RECEIVE_DATE, \n        IFNULL(IFNULL(RIS.BUY_UNIT_AMOUNT, SRIS.BUY_UNIT_AMOUNT), IFNULL(STK.BUY_UNIT_AMOUNT, 0)) AS BUY_UNIT_AMOUNT, \n        IFNULL(\n            RIS.PROFIT_FLAG, \n            IFNULL(\n                SRIS.PROFIT_FLAG,\n                CASE STK.ASSET_TYPE_CODE\n                    WHEN \q01\q THEN 0\n                    WHEN \q02\q THEN 1\n                    ELSE 0\n                END\n            )\n        ) AS RCV_PROFIT_FLAG, \n        \q\q AS PACKING_UNIT_CODE, \n        IFNULL(R.TRADE_CLASSIFY_CODE, SR.TRADE_CLASSIFY_CODE) AS TRADE_CLASSIFY_CODE, \n        IFNULL(R.SUPPLIER_CODE, SR.SUPPLIER_CODE) AS SUPPLIER_CODE, \n        STK.STOCK_STATUS_CODE, \n        STK.STOCK_NOTE, \n        CASE \n            WHEN RIDH.PROCESS_CLASS_CODE = \q14\q \n                THEN IFNULL(DIVIDE_S.SEND_NO, DIVIDE_SH.SEND_NO) \n            WHEN RIDH.PROCESS_CLASS_CODE = \q15\q \n                THEN IFNULL(MACHINING_S.SEND_NO, MACHINING_SH.SEND_NO) \n            ELSE NULL \n            END AS DIVIDE_MACHINING_SEND_NO, \n        CASE \n            WHEN RIDH.PROCESS_CLASS_CODE = \q14\q \n                THEN IFNULL(DIVIDE_S.SELL_RECORD_DATE, DIVIDE_SH.SELL_RECORD_DATE) \n            WHEN RIDH.PROCESS_CLASS_CODE = \q15\q \n                THEN IFNULL(MACHINING_S.SELL_RECORD_DATE, MACHINING_SH.SELL_RECORD_DATE) \n            ELSE NULL \n            END AS DIVIDE_MACHINING_SELL_RECORD_DATE \n    FROM\n        T_RECEIVE_ITEM_DETAIL_HISTORY RIDH \n        LEFT OUTER JOIN T_RECEIVE R \n            ON RIDH.CORP_CODE = R.CORP_CODE \n            AND RIDH.PROCESS_CLASS_CODE = R.PROCESS_CLASS_CODE \n            AND RIDH.RECEIVE_NO = R.RECEIVE_NO \n            AND RIDH.REVISION_NO = R.REVISION_NO - 1 \n        LEFT OUTER JOIN T_RECEIVE_ITEM_SUMMARY RIS \n            ON RIDH.CORP_CODE = RIS.CORP_CODE \n            AND RIDH.PROCESS_CLASS_CODE = RIS.PROCESS_CLASS_CODE \n            AND RIDH.RECEIVE_NO = RIS.RECEIVE_NO \n            AND RIDH.DETAIL_NO = RIS.DETAIL_NO \n            AND RIDH.REVISION_NO = R.REVISION_NO - 1 \n        LEFT OUTER JOIN T_RECEIVE SR \n            ON RIDH.CORP_CODE = SR.CORP_CODE \n            AND RIDH.SRC_PROCESS_CLASS_CODE = SR.PROCESS_CLASS_CODE \n            AND RIDH.SRC_REPORT_NO = SR.RECEIVE_NO \n            AND RIDH.REVISION_NO = SR.REVISION_NO - 1 \n        LEFT OUTER JOIN T_RECEIVE_ITEM_SUMMARY SRIS \n            ON RIDH.CORP_CODE = SRIS.CORP_CODE \n            AND RIDH.SRC_PROCESS_CLASS_CODE = SRIS.PROCESS_CLASS_CODE \n            AND RIDH.SRC_REPORT_NO = SRIS.RECEIVE_NO \n            AND RIDH.SRC_DETAIL_NO = SRIS.DETAIL_NO \n            AND RIDH.REVISION_NO = SR.REVISION_NO - 1 \n        LEFT OUTER JOIN T_SEND S \n            ON RIDH.CORP_CODE = S.CORP_CODE \n            AND RIDH.PROCESS_CLASS_CODE = S.PROCESS_CLASS_CODE \n            AND RIDH.RECEIVE_NO = S.SEND_NO \n            AND RIDH.REVISION_NO = S.REVISION_NO - 1 \n        LEFT OUTER JOIN T_SEND_HISTORY SH \n            ON RIDH.CORP_CODE = SH.CORP_CODE \n            AND RIDH.PROCESS_CLASS_CODE = SH.PROCESS_CLASS_CODE \n            AND RIDH.RECEIVE_NO = SH.SEND_NO \n            AND RIDH.REVISION_NO = SH.REVISION_NO \n        LEFT OUTER JOIN T_STOCK STK \n            ON RIDH.CORP_CODE = STK.CORP_CODE \n            AND RIDH.LATEST_STOCK_NO = STK.STOCK_NO \n            AND RIDH.LATEST_STOCK_HISTORY_NO = STK.STOCK_HISTORY_NO \n        LEFT OUTER JOIN T_ORDER O \n            ON STK.CORP_CODE = O.CORP_CODE \n            AND STK.ORDER_PROCESS_CLASS_CODE = O.PROCESS_CLASS_CODE \n            AND STK.ORDER_NO = O.ORDER_NO \n        LEFT OUTER JOIN T_ORDER_ITEM OI \n            ON STK.CORP_CODE = OI.CORP_CODE \n            AND STK.ORDER_PROCESS_CLASS_CODE = OI.PROCESS_CLASS_CODE \n            AND STK.ORDER_NO = OI.ORDER_NO \n            AND STK.ORDER_ITEM_DETAIL_NO = OI.DETAIL_NO \n        LEFT OUTER JOIN M_ITEM I \n            ON STK.CORP_CODE = I.CORP_CODE \n            AND STK.ITEM_CODE = I.ITEM_CODE \n        LEFT OUTER JOIN T_RECEIVE FRE \n            ON STK.CORP_CODE = FRE.CORP_CODE \n            AND STK.FIRST_RECEIVE_NO = FRE.RECEIVE_NO\n        LEFT OUTER JOIN T_RECEIVE_ITEM_SUMMARY STKRIS \n            ON STK.CORP_CODE = STKRIS.CORP_CODE \n            AND STK.RECEIVE_PROCESS_CLASS_CODE = STKRIS.PROCESS_CLASS_CODE \n            AND STK.RECEIVE_NO = STKRIS.RECEIVE_NO \n            AND STK.RECEIVE_ITEM_SUMMARY_DETAIL_NO = STKRIS.DETAIL_NO \n        LEFT OUTER JOIN T_SEND_ITEM_DETAIL DIVIDE_SID \n            ON STKRIS.CORP_CODE = DIVIDE_SID.CORP_CODE \n            AND STKRIS.ORI_STOCK_NO = DIVIDE_SID.LATEST_STOCK_NO \n            AND STKRIS.ORI_STOCK_HISTORY_NO = DIVIDE_SID.LATEST_STOCK_HISTORY_NO \n            AND STKRIS.PROCESS_CLASS_CODE = \q14\q \n        LEFT OUTER JOIN T_SEND DIVIDE_S \n            ON DIVIDE_SID.CORP_CODE = DIVIDE_S.CORP_CODE \n            AND DIVIDE_SID.PROCESS_CLASS_CODE = DIVIDE_S.PROCESS_CLASS_CODE \n            AND DIVIDE_SID.REPORT_NO = DIVIDE_S.SEND_NO \n            AND DIVIDE_S.REVISION_NO = 0 \n        LEFT OUTER JOIN T_SEND_HISTORY DIVIDE_SH \n            ON DIVIDE_SID.CORP_CODE = DIVIDE_SH.CORP_CODE \n            AND DIVIDE_SID.PROCESS_CLASS_CODE = DIVIDE_SH.PROCESS_CLASS_CODE \n            AND DIVIDE_SID.REPORT_NO = DIVIDE_SH.SEND_NO \n            AND DIVIDE_SH.REVISION_NO = 0 \n            AND DIVIDE_SH.RB_TYPE = \q0\q \n        LEFT OUTER JOIN T_SEND MACHINING_S \n            ON R.CORP_CODE = MACHINING_S.CORP_CODE \n            AND R.MACHINING_SEND_PROCESS_CLASS_CODE = MACHINING_S.PROCESS_CLASS_CODE \n            AND R.MACHINING_SEND_NO = MACHINING_S.SEND_NO \n            AND R.PROCESS_CLASS_CODE = \q15\q \n            AND MACHINING_S.REVISION_NO = 0 \n        LEFT OUTER JOIN T_SEND_HISTORY MACHINING_SH \n            ON R.CORP_CODE = MACHINING_SH.CORP_CODE \n            AND R.MACHINING_SEND_PROCESS_CLASS_CODE = MACHINING_SH.PROCESS_CLASS_CODE \n            AND R.MACHINING_SEND_NO = MACHINING_SH.SEND_NO \n            AND R.PROCESS_CLASS_CODE = \q15\q \n            AND MACHINING_SH.REVISION_NO = 0 \n            AND MACHINING_SH.RB_TYPE = \q0\q \n    WHERE\n        IFNULL(R.RB_TYPE, S.RB_TYPE) = \q1\q\n) \nUNION ALL ( \n    SELECT\n        RID.CORP_CODE, \n        RID.PROCESS_CLASS_CODE, \n        RID.RECEIVE_NO, \n        0 AS REVISION_NO, \n        RID.DETAIL_NO, \n        RID.STOCK_NO, \n        RID.STOCK_HISTORY_NO, \n        RID.SRC_PROCESS_CLASS_CODE, \n        RID.SRC_REPORT_NO, \n        RID.SRC_DETAIL_NO, \n        RID.LATEST_STOCK_NO, \n        RID.LATEST_STOCK_HISTORY_NO, \n        RID.SRC_STOCK_NO, \n        RID.SRC_STOCK_HISTORY_NO, \n        RID.BASE_COST_AMOUNT, \n        RID.BASE_COST_ADD_AMOUNT, \n        RID.BASE_COST_CHARGE_AMOUNT, \n        RID.CREATE_DATETIME, \n        RID.CREATE_USER_CODE, \n        RID.UPDATE_DATETIME, \n        RID.UPDATE_USER_CODE, \n        STK.STOCK_ID, \n        STK.IKO_IN_DT, \n        STK.ASSET_TYPE_CODE AS STK_ASSET_TYPE_CODE, \n        STK.STOCK_QUANTITY, \n        RID.PROCESS_CLASS_CODE AS RCV_PROCESS_CLASS_CODE, \n        CL.CHANGE_LOCATION_NO AS RCV_RECEIVE_NO, \n        0 AS RCV_REVISION_NO, \n        RID.DETAIL_NO AS RCV_DETAIL_NO, \n        \q0\q AS RB_TYPE, \n        NULL AS RCV_ASSET_TYPE_CODE, \n        CL.CHANGE_END_DATE AS BUY_RECORD_DATE, \n        CL.CREATE_DATETIME AS RCV_CREATE_DATETIME, \n        \q0\q AS REGIST_ACCOUNT_FLAG, \n        CL.CHANGE_TO_WAREHOUSE_CODE, \n        STK.ITEM_CODE, \n        I.ITEM_NAME AS MASTER_ITEM_NAME, \n        I.ITEM_SMALL_CLASS_CODE, \n        STK.ORDER_NO, \n        O.ORDER_DATE, \n        STK.STOCK_CONDITION_CODE, \n        STK.LOT_NO, \n        STK.PACKING_NO, \n        STK.COIL_NO, \n        OI.ITEM_NAME, \n        STK.STOCK_UNIT_CODE, \n        STK.FIRST_RECEIVE_NO, \n        FRE.RECEIVE_DATE AS FIRST_RECEIVE_DATE, \n        CL.CHANGE_END_DATE AS RECEIVE_DATE, \n        IFNULL(STK.BUY_UNIT_AMOUNT, 0) AS BUY_UNIT_AMOUNT,\n        CASE STK.ASSET_TYPE_CODE\n            WHEN \q01\q THEN 0\n            WHEN \q02\q THEN 1\n            ELSE 0\n        END AS RCV_PROFIT_FLAG, \n        \q\q AS PACKING_UNIT_CODE, \n        NULL AS TRADE_CLASSIFY_CODE, \n        NULL AS SUPPLIER_CODE, \n        STK.STOCK_STATUS_CODE, \n        STK.STOCK_NOTE, \n        NULL, \n        NULL \n    FROM\n        T_RECEIVE_ITEM_DETAIL RID \n        LEFT OUTER JOIN T_CHANGE_LOCATION CL \n            ON RID.CORP_CODE = CL.CORP_CODE \n            AND RID.RECEIVE_NO = CL.CHANGE_LOCATION_NO \n        LEFT OUTER JOIN T_STOCK STK \n            ON RID.CORP_CODE = STK.CORP_CODE \n            AND RID.LATEST_STOCK_NO = STK.STOCK_NO \n            AND RID.LATEST_STOCK_HISTORY_NO = STK.STOCK_HISTORY_NO \n        LEFT OUTER JOIN T_ORDER O \n            ON STK.CORP_CODE = O.CORP_CODE \n            AND STK.ORDER_PROCESS_CLASS_CODE = O.PROCESS_CLASS_CODE \n            AND STK.ORDER_NO = O.ORDER_NO \n        LEFT OUTER JOIN T_ORDER_ITEM OI \n            ON STK.CORP_CODE = OI.CORP_CODE \n            AND STK.ORDER_PROCESS_CLASS_CODE = OI.PROCESS_CLASS_CODE \n            AND STK.ORDER_NO = OI.ORDER_NO \n            AND STK.ORDER_ITEM_DETAIL_NO = OI.DETAIL_NO \n        LEFT OUTER JOIN T_RECEIVE STKR \n            ON STK.CORP_CODE = STKR.CORP_CODE \n            AND STK.RECEIVE_PROCESS_CLASS_CODE = STKR.PROCESS_CLASS_CODE \n            AND STK.RECEIVE_NO = STKR.RECEIVE_NO \n        LEFT OUTER JOIN T_RECEIVE_ITEM_SUMMARY STKRIS \n            ON STK.CORP_CODE = STKRIS.CORP_CODE \n            AND STK.RECEIVE_PROCESS_CLASS_CODE = STKRIS.PROCESS_CLASS_CODE \n            AND STK.RECEIVE_NO = STKRIS.RECEIVE_NO \n            AND STK.RECEIVE_ITEM_SUMMARY_DETAIL_NO = STKRIS.DETAIL_NO \n        LEFT OUTER JOIN M_ITEM I \n            ON STK.CORP_CODE = I.CORP_CODE \n            AND STK.ITEM_CODE = I.ITEM_CODE \n        LEFT OUTER JOIN T_RECEIVE FRE \n            ON STK.CORP_CODE = FRE.CORP_CODE \n            AND STK.FIRST_RECEIVE_NO = FRE.RECEIVE_NO \n    WHERE\n        RID.PROCESS_CLASS_CODE = \q41\q\n)\n
