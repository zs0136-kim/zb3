LName=出庫商品明細ビュー
Comment=出庫商品明細の履歴を含めたビュー。月次在庫バッチで利用する。\nREGIST_ACCOUNT_FLAG\n⇒経理連携する取引分類は「1」を設定\n⇒取引分類が調整かつ確定区分が済の場合は「1」を設定\n⇒処理種別が小割、加工の場合は「1」を設定
Query=( \n    SELECT\n        SID.CORP_CODE, \n        SID.PROCESS_CLASS_CODE, \n        SID.REPORT_NO, \n        S.REVISION_NO, \n        SID.DETAIL_NO, \n        SID.STOCK_NO, \n        SID.STOCK_HISTORY_NO, \n        SID.LATEST_STOCK_NO, \n        SID.LATEST_STOCK_HISTORY_NO, \n        CASE \n            WHEN SID.PROCESS_CLASS_CODE IN (\q35\q) \n                THEN SID.SELL_AMOUNT * - 1 \n            ELSE SID.SELL_AMOUNT \n            END AS SELL_AMOUNT, \n        CASE \n            WHEN SID.PROCESS_CLASS_CODE IN (\q35\q) \n                THEN SID.SELL_ADD_AMOUNT * - 1 \n            ELSE SID.SELL_ADD_AMOUNT \n            END AS SELL_ADD_AMOUNT, \n        SID.CREATE_DATETIME, \n        SID.CREATE_USER_CODE, \n        SID.UPDATE_DATETIME, \n        SID.UPDATE_USER_CODE, \n        STK.STOCK_ID, \n        STK.ASSET_TYPE_CODE AS STK_ASSET_TYPE_CODE, \n        CASE \n            WHEN SID.PROCESS_CLASS_CODE IN (\q36\q) \n                THEN STK.STOCK_QUANTITY * - 1 \n            ELSE STK.STOCK_QUANTITY \n            END AS STOCK_QUANTITY, \n        CASE \n            WHEN SID.PROCESS_CLASS_CODE IN (\q35\q, \q36\q) \n                THEN STK.SELL_QUANTITY * - 1 \n            ELSE STK.SELL_QUANTITY \n            END AS SELL_QUANTITY, \n        S.RB_TYPE, \n        S.FIX_TYPE, \n        S.SEND_DATE, \n        S.SELL_RECORD_DATE, \n        NULL AS CHANGE_START_DATE, \n        S.TRADE_CLASSIFY_CODE, \n        S.CUSTOMER_CODE, \n        S.WAREHOUSE_CODE, \n        S.CREATE_DATETIME AS SND_CREATE_DATETIME, \n        CASE \n            WHEN S.TRADE_CLASSIFY_CODE IS NULL \n                THEN \q1\q \n            WHEN S.TRADE_CLASSIFY_CODE IN ( \n                \qINV-A3\q, \n                \qINV-C1\q, \n                \qINV-C2\q, \n                \qINV-E\q, \n                \qINV-I\q, \n                \qINV-I2\q, \n                \qINV-M1\q, \n                \qINV-M2\q, \n                \qINV-R1\q, \n                \qINV-R2\q, \n                \qINV-R3\q, \n                \qINV-R4\q, \n                \qINV-T\q, \n                \qSTK-R1\q, \n                \qSTK-R2\q\n            ) \n                THEN \q1\q \n            ELSE \q0\q \n            END AS REGIST_ACCOUNT_FLAG, \n        SIS.SELL_UNIT_AMOUNT, \n        SIS.PROFIT_FLAG, \n        CASE \n            WHEN SID.PROCESS_CLASS_CODE = \q33\q \n                THEN IFNULL(DIVIDE_R.RECEIVE_NO, DIVIDE_RH.RECEIVE_NO) \n            WHEN SID.PROCESS_CLASS_CODE = \q34\q \n                THEN IFNULL(MACHINING_R.RECEIVE_NO, MACHINING_RH.RECEIVE_NO) \n            ELSE NULL \n            END AS DIVIDE_MACHINING_RECEIVE_NO, \n        CASE \n            WHEN SID.PROCESS_CLASS_CODE = \q33\q \n                THEN IFNULL(DIVIDE_R.BUY_RECORD_DATE, DIVIDE_RH.BUY_RECORD_DATE) \n            WHEN SID.PROCESS_CLASS_CODE = \q34\q \n                THEN IFNULL(MACHINING_R.BUY_RECORD_DATE, MACHINING_RH.BUY_RECORD_DATE) \n            ELSE NULL \n            END AS DIVIDE_MACHINING_BUY_RECORD_DATE \n    FROM\n        T_SEND_ITEM_DETAIL SID \n        LEFT OUTER JOIN T_SEND S \n            ON SID.CORP_CODE = S.CORP_CODE \n            AND SID.PROCESS_CLASS_CODE = S.PROCESS_CLASS_CODE \n            AND SID.REPORT_NO = S.SEND_NO \n        LEFT OUTER JOIN T_SEND_ITEM_SUMMARY SIS \n            ON SID.CORP_CODE = SIS.CORP_CODE \n            AND SID.PROCESS_CLASS_CODE = SIS.PROCESS_CLASS_CODE \n            AND SID.REPORT_NO = SIS.REPORT_NO \n            AND SID.DETAIL_NO = SIS.DETAIL_NO \n        LEFT OUTER JOIN T_STOCK STK \n            ON SID.CORP_CODE = STK.CORP_CODE \n            AND SID.LATEST_STOCK_NO = STK.STOCK_NO \n            AND SID.LATEST_STOCK_HISTORY_NO = STK.STOCK_HISTORY_NO \n        LEFT OUTER JOIN T_RECEIVE_ITEM_SUMMARY DIVIDE_RIS \n            ON STK.CORP_CODE = DIVIDE_RIS.CORP_CODE \n            AND STK.STOCK_NO = DIVIDE_RIS.ORI_STOCK_NO \n            AND STK.STOCK_HISTORY_NO = DIVIDE_RIS.ORI_STOCK_HISTORY_NO \n            AND SID.PROCESS_CLASS_CODE = \q33\q \n        LEFT OUTER JOIN T_RECEIVE DIVIDE_R \n            ON DIVIDE_RIS.CORP_CODE = DIVIDE_R.CORP_CODE \n            AND DIVIDE_RIS.PROCESS_CLASS_CODE = DIVIDE_R.PROCESS_CLASS_CODE \n            AND DIVIDE_RIS.RECEIVE_NO = DIVIDE_R.RECEIVE_NO \n            AND DIVIDE_R.REVISION_NO = 0 \n        LEFT OUTER JOIN T_RECEIVE_HISTORY DIVIDE_RH \n            ON DIVIDE_RIS.CORP_CODE = DIVIDE_RH.CORP_CODE \n            AND DIVIDE_RIS.PROCESS_CLASS_CODE = DIVIDE_RH.PROCESS_CLASS_CODE \n            AND DIVIDE_RIS.RECEIVE_NO = DIVIDE_RH.RECEIVE_NO \n            AND DIVIDE_RH.REVISION_NO = 0 \n            AND DIVIDE_RH.RB_TYPE = \q0\q \n        LEFT OUTER JOIN T_RECEIVE MACHINING_R \n            ON S.CORP_CODE = MACHINING_R.CORP_CODE \n            AND S.PROCESS_CLASS_CODE = MACHINING_R.MACHINING_SEND_PROCESS_CLASS_CODE \n            AND S.SEND_NO = MACHINING_R.MACHINING_SEND_NO \n            AND S.PROCESS_CLASS_CODE = \q34\q \n            AND MACHINING_R.REVISION_NO = 0 \n        LEFT OUTER JOIN T_RECEIVE_HISTORY MACHINING_RH \n            ON S.CORP_CODE = MACHINING_RH.CORP_CODE \n            AND S.PROCESS_CLASS_CODE = MACHINING_RH.MACHINING_SEND_PROCESS_CLASS_CODE \n            AND S.SEND_NO = MACHINING_RH.MACHINING_SEND_NO \n            AND S.PROCESS_CLASS_CODE = \q34\q \n            AND MACHINING_RH.REVISION_NO = 0 \n            AND MACHINING_RH.RB_TYPE = \q0\q \n    WHERE\n        S.RB_TYPE = \q0\q \n        AND SID.PROCESS_CLASS_CODE NOT IN (\q38\q)\n) \nUNION ALL ( \n    SELECT\n        SIDH.CORP_CODE, \n        SIDH.PROCESS_CLASS_CODE, \n        SIDH.REPORT_NO, \n        SIDH.REVISION_NO, \n        SIDH.DETAIL_NO, \n        SIDH.STOCK_NO, \n        SIDH.STOCK_HISTORY_NO, \n        SIDH.LATEST_STOCK_NO, \n        SIDH.LATEST_STOCK_HISTORY_NO, \n        CASE \n            WHEN SIDH.PROCESS_CLASS_CODE IN (\q35\q) \n                THEN SIDH.SELL_AMOUNT \n            ELSE SIDH.SELL_AMOUNT * - 1 \n            END AS SELL_AMOUNT, \n        CASE \n            WHEN SIDH.PROCESS_CLASS_CODE IN (\q35\q, \q36\q) \n                THEN SIDH.SELL_ADD_AMOUNT \n            ELSE SIDH.SELL_ADD_AMOUNT * - 1 \n            END AS SELL_ADD_AMOUNT, \n        SIDH.CREATE_DATETIME, \n        SIDH.CREATE_USER_CODE, \n        SIDH.UPDATE_DATETIME, \n        SIDH.UPDATE_USER_CODE, \n        STK.STOCK_ID, \n        STK.ASSET_TYPE_CODE AS STK_ASSET_TYPE_CODE, \n        CASE \n            WHEN SIDH.PROCESS_CLASS_CODE IN (\q36\q) \n                THEN STK.STOCK_QUANTITY \n            ELSE STK.STOCK_QUANTITY * - 1 \n            END AS STOCK_QUANTITY, \n        CASE \n            WHEN SIDH.PROCESS_CLASS_CODE IN (\q35\q, \q36\q) \n                THEN STK.SELL_QUANTITY \n            ELSE STK.SELL_QUANTITY * - 1 \n            END AS SELL_QUANTITY, \n        S.RB_TYPE, \n        S.FIX_TYPE, \n        S.SEND_DATE, \n        S.SELL_RECORD_DATE, \n        NULL AS CHANGE_START_DATE, \n        S.TRADE_CLASSIFY_CODE, \n        S.CUSTOMER_CODE, \n        S.WAREHOUSE_CODE, \n        S.CREATE_DATETIME AS SND_CREATE_DATETIME, \n        CASE \n            WHEN S.TRADE_CLASSIFY_CODE IS NULL \n                THEN \q1\q \n            WHEN S.TRADE_CLASSIFY_CODE IN ( \n                \qINV-A3\q, \n                \qINV-C1\q, \n                \qINV-C2\q, \n                \qINV-E\q, \n                \qINV-I\q, \n                \qINV-I2\q, \n                \qINV-M1\q, \n                \qINV-M2\q, \n                \qINV-R1\q, \n                \qINV-R2\q, \n                \qINV-R3\q, \n                \qINV-R4\q, \n                \qINV-T\q, \n                \qSTK-R1\q, \n                \qSTK-R2\q\n            ) \n                THEN \q1\q \n            ELSE \q0\q \n            END AS REGIST_ACCOUNT_FLAG, \n        SIS.SELL_UNIT_AMOUNT, \n        SIS.PROFIT_FLAG, \n        CASE \n            WHEN SIDH.PROCESS_CLASS_CODE = \q33\q \n                THEN IFNULL(DIVIDE_R.RECEIVE_NO, DIVIDE_RH.RECEIVE_NO) \n            WHEN SIDH.PROCESS_CLASS_CODE = \q34\q \n                THEN IFNULL(MACHINING_R.RECEIVE_NO, MACHINING_RH.RECEIVE_NO) \n            ELSE NULL \n            END AS DIVIDE_MACHINING_RECEIVE_NO, \n        CASE \n            WHEN SIDH.PROCESS_CLASS_CODE = \q33\q \n                THEN IFNULL(DIVIDE_R.BUY_RECORD_DATE, DIVIDE_RH.BUY_RECORD_DATE) \n            WHEN SIDH.PROCESS_CLASS_CODE = \q34\q \n                THEN IFNULL(MACHINING_R.BUY_RECORD_DATE, MACHINING_RH.BUY_RECORD_DATE) \n            ELSE NULL \n            END AS DIVIDE_MACHINING_BUY_RECORD_DATE \n    FROM\n        T_SEND_ITEM_DETAIL_HISTORY SIDH \n        LEFT OUTER JOIN T_SEND S \n            ON SIDH.CORP_CODE = S.CORP_CODE \n            AND SIDH.PROCESS_CLASS_CODE = S.PROCESS_CLASS_CODE \n            AND SIDH.REPORT_NO = S.SEND_NO \n            AND SIDH.REVISION_NO = S.REVISION_NO - 1 \n        LEFT OUTER JOIN T_SEND_ITEM_SUMMARY SIS \n            ON SIDH.CORP_CODE = SIS.CORP_CODE \n            AND SIDH.PROCESS_CLASS_CODE = SIS.PROCESS_CLASS_CODE \n            AND SIDH.REPORT_NO = SIS.REPORT_NO \n            AND SIDH.DETAIL_NO = SIS.DETAIL_NO \n            AND SIDH.REVISION_NO = S.REVISION_NO - 1 \n        LEFT OUTER JOIN T_STOCK STK \n            ON SIDH.CORP_CODE = STK.CORP_CODE \n            AND SIDH.LATEST_STOCK_NO = STK.STOCK_NO \n            AND SIDH.LATEST_STOCK_HISTORY_NO = STK.STOCK_HISTORY_NO \n        LEFT OUTER JOIN T_RECEIVE_ITEM_SUMMARY DIVIDE_RIS \n            ON STK.CORP_CODE = DIVIDE_RIS.CORP_CODE \n            AND STK.STOCK_NO = DIVIDE_RIS.ORI_STOCK_NO \n            AND STK.STOCK_HISTORY_NO = DIVIDE_RIS.ORI_STOCK_HISTORY_NO \n            AND SIDH.PROCESS_CLASS_CODE = \q33\q \n        LEFT OUTER JOIN T_RECEIVE DIVIDE_R \n            ON DIVIDE_RIS.CORP_CODE = DIVIDE_R.CORP_CODE \n            AND DIVIDE_RIS.PROCESS_CLASS_CODE = DIVIDE_R.PROCESS_CLASS_CODE \n            AND DIVIDE_RIS.RECEIVE_NO = DIVIDE_R.RECEIVE_NO \n            AND DIVIDE_R.REVISION_NO = 0 \n        LEFT OUTER JOIN T_RECEIVE_HISTORY DIVIDE_RH \n            ON DIVIDE_RIS.CORP_CODE = DIVIDE_RH.CORP_CODE \n            AND DIVIDE_RIS.PROCESS_CLASS_CODE = DIVIDE_RH.PROCESS_CLASS_CODE \n            AND DIVIDE_RIS.RECEIVE_NO = DIVIDE_RH.RECEIVE_NO \n            AND DIVIDE_RH.REVISION_NO = 0 \n            AND DIVIDE_RH.RB_TYPE = \q0\q \n        LEFT OUTER JOIN T_RECEIVE MACHINING_R \n            ON S.CORP_CODE = MACHINING_R.CORP_CODE \n            AND S.PROCESS_CLASS_CODE = MACHINING_R.MACHINING_SEND_PROCESS_CLASS_CODE \n            AND S.SEND_NO = MACHINING_R.MACHINING_SEND_NO \n            AND S.PROCESS_CLASS_CODE = \q34\q \n            AND MACHINING_R.REVISION_NO = 0 \n        LEFT OUTER JOIN T_RECEIVE_HISTORY MACHINING_RH \n            ON S.CORP_CODE = MACHINING_RH.CORP_CODE \n            AND S.PROCESS_CLASS_CODE = MACHINING_RH.MACHINING_SEND_PROCESS_CLASS_CODE \n            AND S.SEND_NO = MACHINING_RH.MACHINING_SEND_NO \n            AND S.PROCESS_CLASS_CODE = \q34\q \n            AND MACHINING_RH.REVISION_NO = 0 \n            AND MACHINING_RH.RB_TYPE = \q0\q \n    WHERE\n        S.RB_TYPE = \q1\q \n        AND SIDH.PROCESS_CLASS_CODE NOT IN (\q38\q)\n) \nUNION ALL ( \n    SELECT\n        SIDH.CORP_CODE, \n        SIDH.PROCESS_CLASS_CODE, \n        SIDH.REPORT_NO, \n        SIDH.REVISION_NO, \n        SIDH.DETAIL_NO, \n        SIDH.STOCK_NO, \n        SIDH.STOCK_HISTORY_NO, \n        SIDH.LATEST_STOCK_NO, \n        SIDH.LATEST_STOCK_HISTORY_NO, \n        CASE \n            WHEN SIDH.PROCESS_CLASS_CODE IN (\q35\q) \n                THEN SIDH.SELL_AMOUNT * - 1 \n            ELSE SIDH.SELL_AMOUNT \n            END AS SELL_AMOUNT, \n        CASE \n            WHEN SIDH.PROCESS_CLASS_CODE IN (\q35\q) \n                THEN SIDH.SELL_ADD_AMOUNT * - 1 \n            ELSE SIDH.SELL_ADD_AMOUNT \n            END AS SELL_ADD_AMOUNT, \n        SIDH.CREATE_DATETIME, \n        SIDH.CREATE_USER_CODE, \n        SIDH.UPDATE_DATETIME, \n        SIDH.UPDATE_USER_CODE, \n        STK.STOCK_ID, \n        STK.ASSET_TYPE_CODE AS STK_ASSET_TYPE_CODE, \n        CASE \n            WHEN SIDH.PROCESS_CLASS_CODE IN (\q36\q) \n                THEN STK.STOCK_QUANTITY * - 1 \n            ELSE STK.STOCK_QUANTITY \n            END AS STOCK_QUANTITY, \n        CASE \n            WHEN SIDH.PROCESS_CLASS_CODE IN (\q35\q, \q36\q) \n                THEN STK.SELL_QUANTITY * - 1 \n            ELSE STK.SELL_QUANTITY \n            END AS SELL_QUANTITY, \n        SH.RB_TYPE, \n        SH.FIX_TYPE, \n        SH.SEND_DATE, \n        SH.SELL_RECORD_DATE, \n        NULL AS CHANGE_START_DATE, \n        SH.TRADE_CLASSIFY_CODE, \n        SH.CUSTOMER_CODE, \n        SH.WAREHOUSE_CODE, \n        SH.CREATE_DATETIME AS SND_CREATE_DATETIME, \n        CASE \n            WHEN SH.TRADE_CLASSIFY_CODE IS NULL \n                THEN \q1\q \n            WHEN SH.TRADE_CLASSIFY_CODE IN ( \n                \qINV-A3\q, \n                \qINV-C1\q, \n                \qINV-C2\q, \n                \qINV-E\q, \n                \qINV-I\q, \n                \qINV-I2\q, \n                \qINV-M1\q, \n                \qINV-M2\q, \n                \qINV-R1\q, \n                \qINV-R2\q, \n                \qINV-R3\q, \n                \qINV-R4\q, \n                \qINV-T\q, \n                \qSTK-R1\q, \n                \qSTK-R2\q\n            ) \n                THEN \q1\q \n            ELSE \q0\q \n            END AS REGIST_ACCOUNT_FLAG, \n        SISH.SELL_UNIT_AMOUNT, \n        SISH.PROFIT_FLAG, \n        CASE \n            WHEN SIDH.PROCESS_CLASS_CODE = \q33\q \n                THEN IFNULL(DIVIDE_R.RECEIVE_NO, DIVIDE_RH.RECEIVE_NO) \n            WHEN SIDH.PROCESS_CLASS_CODE = \q34\q \n                THEN IFNULL(MACHINING_R.RECEIVE_NO, MACHINING_RH.RECEIVE_NO) \n            ELSE NULL \n            END AS DIVIDE_MACHINING_RECEIVE_NO, \n        CASE \n            WHEN SIDH.PROCESS_CLASS_CODE = \q33\q \n                THEN IFNULL(DIVIDE_R.BUY_RECORD_DATE, DIVIDE_RH.BUY_RECORD_DATE) \n            WHEN SIDH.PROCESS_CLASS_CODE = \q34\q \n                THEN IFNULL(MACHINING_R.BUY_RECORD_DATE, MACHINING_RH.BUY_RECORD_DATE) \n            ELSE NULL \n            END AS DIVIDE_MACHINING_BUY_RECORD_DATE \n    FROM\n        T_SEND_ITEM_DETAIL_HISTORY SIDH \n        LEFT OUTER JOIN T_SEND_HISTORY SH \n            ON SIDH.CORP_CODE = SH.CORP_CODE \n            AND SIDH.PROCESS_CLASS_CODE = SH.PROCESS_CLASS_CODE \n            AND SIDH.REPORT_NO = SH.SEND_NO \n            AND SIDH.REVISION_NO = SH.REVISION_NO \n        LEFT OUTER JOIN T_SEND_ITEM_SUMMARY_HISTORY SISH \n            ON SIDH.CORP_CODE = SISH.CORP_CODE \n            AND SIDH.PROCESS_CLASS_CODE = SISH.PROCESS_CLASS_CODE \n            AND SIDH.REPORT_NO = SISH.REPORT_NO \n            AND SIDH.DETAIL_NO = SISH.DETAIL_NO \n            AND SIDH.REVISION_NO = SISH.REVISION_NO \n        LEFT OUTER JOIN T_STOCK STK \n            ON SIDH.CORP_CODE = STK.CORP_CODE \n            AND SIDH.LATEST_STOCK_NO = STK.STOCK_NO \n            AND SIDH.LATEST_STOCK_HISTORY_NO = STK.STOCK_HISTORY_NO \n        LEFT OUTER JOIN T_RECEIVE_ITEM_SUMMARY DIVIDE_RIS \n            ON STK.CORP_CODE = DIVIDE_RIS.CORP_CODE \n            AND STK.STOCK_NO = DIVIDE_RIS.ORI_STOCK_NO \n            AND STK.STOCK_HISTORY_NO = DIVIDE_RIS.ORI_STOCK_HISTORY_NO \n            AND SIDH.PROCESS_CLASS_CODE = \q33\q \n        LEFT OUTER JOIN T_RECEIVE DIVIDE_R \n            ON DIVIDE_RIS.CORP_CODE = DIVIDE_R.CORP_CODE \n            AND DIVIDE_RIS.PROCESS_CLASS_CODE = DIVIDE_R.PROCESS_CLASS_CODE \n            AND DIVIDE_RIS.RECEIVE_NO = DIVIDE_R.RECEIVE_NO \n            AND DIVIDE_R.REVISION_NO = 0 \n        LEFT OUTER JOIN T_RECEIVE_HISTORY DIVIDE_RH \n            ON DIVIDE_RIS.CORP_CODE = DIVIDE_RH.CORP_CODE \n            AND DIVIDE_RIS.PROCESS_CLASS_CODE = DIVIDE_RH.PROCESS_CLASS_CODE \n            AND DIVIDE_RIS.RECEIVE_NO = DIVIDE_RH.RECEIVE_NO \n            AND DIVIDE_RH.REVISION_NO = 0 \n            AND DIVIDE_RH.RB_TYPE = \q0\q \n        LEFT OUTER JOIN T_RECEIVE MACHINING_R \n            ON SH.CORP_CODE = MACHINING_R.CORP_CODE \n            AND SH.PROCESS_CLASS_CODE = MACHINING_R.MACHINING_SEND_PROCESS_CLASS_CODE \n            AND SH.SEND_NO = MACHINING_R.MACHINING_SEND_NO \n            AND SH.PROCESS_CLASS_CODE = \q34\q \n            AND MACHINING_R.REVISION_NO = 0 \n        LEFT OUTER JOIN T_RECEIVE_HISTORY MACHINING_RH \n            ON SH.CORP_CODE = MACHINING_RH.CORP_CODE \n            AND SH.PROCESS_CLASS_CODE = MACHINING_RH.MACHINING_SEND_PROCESS_CLASS_CODE \n            AND SH.SEND_NO = MACHINING_RH.MACHINING_SEND_NO \n            AND SH.PROCESS_CLASS_CODE = \q34\q \n            AND MACHINING_RH.REVISION_NO = 0 \n            AND MACHINING_RH.RB_TYPE = \q0\q \n    WHERE\n        SH.RB_TYPE = \q0\q \n        AND SIDH.PROCESS_CLASS_CODE NOT IN (\q38\q)\n) \nUNION ALL ( \n    SELECT\n        SIDH.CORP_CODE, \n        SIDH.PROCESS_CLASS_CODE, \n        SIDH.REPORT_NO, \n        SIDH.REVISION_NO, \n        SIDH.DETAIL_NO, \n        SIDH.STOCK_NO, \n        SIDH.STOCK_HISTORY_NO, \n        SIDH.LATEST_STOCK_NO, \n        SIDH.LATEST_STOCK_HISTORY_NO, \n        CASE \n            WHEN SIDH.PROCESS_CLASS_CODE IN (\q35\q) \n                THEN SIDH.SELL_AMOUNT \n            ELSE SIDH.SELL_AMOUNT * - 1 \n            END AS SELL_AMOUNT, \n        CASE \n            WHEN SIDH.PROCESS_CLASS_CODE IN (\q35\q, \q36\q) \n                THEN SIDH.SELL_ADD_AMOUNT \n            ELSE SIDH.SELL_ADD_AMOUNT * - 1 \n            END AS SELL_ADD_AMOUNT, \n        SIDH.CREATE_DATETIME, \n        SIDH.CREATE_USER_CODE, \n        SIDH.UPDATE_DATETIME, \n        SIDH.UPDATE_USER_CODE, \n        STK.STOCK_ID, \n        STK.ASSET_TYPE_CODE AS STK_ASSET_TYPE_CODE, \n        CASE \n            WHEN SIDH.PROCESS_CLASS_CODE IN (\q36\q) \n                THEN STK.STOCK_QUANTITY \n            ELSE STK.STOCK_QUANTITY * - 1 \n            END AS STOCK_QUANTITY, \n        CASE \n            WHEN SIDH.PROCESS_CLASS_CODE IN (\q35\q, \q36\q) \n                THEN STK.SELL_QUANTITY \n            ELSE STK.SELL_QUANTITY * - 1 \n            END AS SELL_QUANTITY, \n        SH.RB_TYPE, \n        SH.FIX_TYPE, \n        SH.SEND_DATE, \n        SH.SELL_RECORD_DATE, \n        NULL AS CHANGE_START_DATE, \n        SH.TRADE_CLASSIFY_CODE, \n        SH.CUSTOMER_CODE, \n        SH.WAREHOUSE_CODE, \n        SH.CREATE_DATETIME AS SND_CREATE_DATETIME, \n        CASE \n            WHEN SH.TRADE_CLASSIFY_CODE IS NULL \n                THEN \q1\q \n            WHEN SH.TRADE_CLASSIFY_CODE IN ( \n                \qINV-A3\q, \n                \qINV-C1\q, \n                \qINV-C2\q, \n                \qINV-E\q, \n                \qINV-I\q, \n                \qINV-I2\q, \n                \qINV-M1\q, \n                \qINV-M2\q, \n                \qINV-R1\q, \n                \qINV-R2\q, \n                \qINV-R3\q, \n                \qINV-R4\q, \n                \qINV-T\q, \n                \qSTK-R1\q, \n                \qSTK-R2\q\n            ) \n                THEN \q1\q \n            ELSE \q0\q \n            END AS REGIST_ACCOUNT_FLAG, \n        SISH.SELL_UNIT_AMOUNT, \n        SISH.PROFIT_FLAG, \n        CASE \n            WHEN SIDH.PROCESS_CLASS_CODE = \q33\q \n                THEN IFNULL(DIVIDE_R.RECEIVE_NO, DIVIDE_RH.RECEIVE_NO) \n            WHEN SIDH.PROCESS_CLASS_CODE = \q34\q \n                THEN IFNULL(MACHINING_R.RECEIVE_NO, MACHINING_RH.RECEIVE_NO) \n            ELSE NULL \n            END AS DIVIDE_MACHINING_RECEIVE_NO, \n        CASE \n            WHEN SIDH.PROCESS_CLASS_CODE = \q33\q \n                THEN IFNULL(DIVIDE_R.BUY_RECORD_DATE, DIVIDE_RH.BUY_RECORD_DATE) \n            WHEN SIDH.PROCESS_CLASS_CODE = \q34\q \n                THEN IFNULL(MACHINING_R.BUY_RECORD_DATE, MACHINING_RH.BUY_RECORD_DATE) \n            ELSE NULL \n            END AS DIVIDE_MACHINING_BUY_RECORD_DATE \n    FROM\n        T_SEND_ITEM_DETAIL_HISTORY SIDH \n        LEFT OUTER JOIN T_SEND_HISTORY SH \n            ON SIDH.CORP_CODE = SH.CORP_CODE \n            AND SIDH.PROCESS_CLASS_CODE = SH.PROCESS_CLASS_CODE \n            AND SIDH.REPORT_NO = SH.SEND_NO \n            AND SIDH.REVISION_NO = SH.REVISION_NO \n        LEFT OUTER JOIN T_SEND_ITEM_SUMMARY_HISTORY SISH \n            ON SIDH.CORP_CODE = SISH.CORP_CODE \n            AND SIDH.PROCESS_CLASS_CODE = SISH.PROCESS_CLASS_CODE \n            AND SIDH.REPORT_NO = SISH.REPORT_NO \n            AND SIDH.DETAIL_NO = SISH.DETAIL_NO \n            AND SIDH.REVISION_NO = SISH.REVISION_NO \n        LEFT OUTER JOIN T_STOCK STK \n            ON SIDH.CORP_CODE = STK.CORP_CODE \n            AND SIDH.LATEST_STOCK_NO = STK.STOCK_NO \n            AND SIDH.LATEST_STOCK_HISTORY_NO = STK.STOCK_HISTORY_NO \n        LEFT OUTER JOIN T_RECEIVE_ITEM_SUMMARY DIVIDE_RIS \n            ON STK.CORP_CODE = DIVIDE_RIS.CORP_CODE \n            AND STK.STOCK_NO = DIVIDE_RIS.ORI_STOCK_NO \n            AND STK.STOCK_HISTORY_NO = DIVIDE_RIS.ORI_STOCK_HISTORY_NO \n            AND SIDH.PROCESS_CLASS_CODE = \q33\q \n        LEFT OUTER JOIN T_RECEIVE DIVIDE_R \n            ON DIVIDE_RIS.CORP_CODE = DIVIDE_R.CORP_CODE \n            AND DIVIDE_RIS.PROCESS_CLASS_CODE = DIVIDE_R.PROCESS_CLASS_CODE \n            AND DIVIDE_RIS.RECEIVE_NO = DIVIDE_R.RECEIVE_NO \n            AND DIVIDE_R.REVISION_NO = 0 \n        LEFT OUTER JOIN T_RECEIVE_HISTORY DIVIDE_RH \n            ON DIVIDE_RIS.CORP_CODE = DIVIDE_RH.CORP_CODE \n            AND DIVIDE_RIS.PROCESS_CLASS_CODE = DIVIDE_RH.PROCESS_CLASS_CODE \n            AND DIVIDE_RIS.RECEIVE_NO = DIVIDE_RH.RECEIVE_NO \n            AND DIVIDE_RH.REVISION_NO = 0 \n            AND DIVIDE_RH.RB_TYPE = \q0\q \n        LEFT OUTER JOIN T_RECEIVE MACHINING_R \n            ON SH.CORP_CODE = MACHINING_R.CORP_CODE \n            AND SH.PROCESS_CLASS_CODE = MACHINING_R.MACHINING_SEND_PROCESS_CLASS_CODE \n            AND SH.SEND_NO = MACHINING_R.MACHINING_SEND_NO \n            AND SH.PROCESS_CLASS_CODE = \q34\q \n            AND MACHINING_R.REVISION_NO = 0 \n        LEFT OUTER JOIN T_RECEIVE_HISTORY MACHINING_RH \n            ON SH.CORP_CODE = MACHINING_RH.CORP_CODE \n            AND SH.PROCESS_CLASS_CODE = MACHINING_RH.MACHINING_SEND_PROCESS_CLASS_CODE \n            AND SH.SEND_NO = MACHINING_RH.MACHINING_SEND_NO \n            AND SH.PROCESS_CLASS_CODE = \q34\q \n            AND MACHINING_RH.REVISION_NO = 0 \n            AND MACHINING_RH.RB_TYPE = \q0\q \n    WHERE\n        SH.RB_TYPE = \q1\q \n        AND SIDH.PROCESS_CLASS_CODE NOT IN (\q38\q)\n) \nUNION ALL ( \n    SELECT\n        SID.CORP_CODE, \n        SID.PROCESS_CLASS_CODE, \n        SID.REPORT_NO, \n        \q0\q AS REVISION_NO, \n        SID.DETAIL_NO, \n        SID.STOCK_NO, \n        SID.STOCK_HISTORY_NO, \n        SID.LATEST_STOCK_NO, \n        SID.LATEST_STOCK_HISTORY_NO, \n        SID.SELL_AMOUNT, \n        SID.SELL_ADD_AMOUNT, \n        SID.CREATE_DATETIME, \n        SID.CREATE_USER_CODE, \n        SID.UPDATE_DATETIME, \n        SID.UPDATE_USER_CODE, \n        STK.STOCK_ID, \n        STK.ASSET_TYPE_CODE AS STK_ASSET_TYPE_CODE, \n        STK.STOCK_QUANTITY, \n        STK.SELL_QUANTITY, \n        \q0\q AS RB_TYPE, \n        \q2\q AS FIX_TYPE, \n        CL.CHANGE_END_DATE AS SEND_DATE, \n        CL.CHANGE_END_DATE AS SELL_RECORD_DATE, \n        CL.CHANGE_START_DATE, \n        NULL AS TRADE_CLASSIFY_CODE, \n        NULL AS CUSTOMER_CODE, \n        CL.CHANGE_FROM_WAREHOUSE_CODE AS WAREHOUSE_CODE, \n        CL.CREATE_DATETIME AS SND_CREATE_DATETIME, \n        \q0\q AS REGIST_ACCOUNT_FLAG, \n        SIS.SELL_UNIT_AMOUNT, \n        SIS.PROFIT_FLAG, \n        NULL, \n        NULL \n    FROM\n        T_SEND_ITEM_DETAIL SID \n        LEFT OUTER JOIN T_CHANGE_LOCATION CL \n            ON SID.CORP_CODE = CL.CORP_CODE \n            AND SID.REPORT_NO = CL.CHANGE_LOCATION_NO \n        LEFT OUTER JOIN T_SEND_ITEM_SUMMARY SIS \n            ON SID.CORP_CODE = SIS.CORP_CODE \n            AND SID.PROCESS_CLASS_CODE = SIS.PROCESS_CLASS_CODE \n            AND SID.REPORT_NO = SIS.REPORT_NO \n            AND SID.DETAIL_NO = SIS.DETAIL_NO \n        LEFT OUTER JOIN T_STOCK STK \n            ON SID.CORP_CODE = STK.CORP_CODE \n            AND SID.LATEST_STOCK_NO = STK.STOCK_NO \n            AND SID.LATEST_STOCK_HISTORY_NO = STK.STOCK_HISTORY_NO \n    WHERE\n        SID.PROCESS_CLASS_CODE = \q41\q\n)\n
